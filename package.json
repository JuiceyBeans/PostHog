{
  "name": "posthog-discord-bot",
  "version": "1.0.0",
  "description": "Discord bot for PostHog - developed entirely by Claude",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && cp src/config/triggers.json dist/config/triggers.json",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript"
  ],
  "author": "Claude",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
